<p id="notice"><%= notice %></p>

<h1>Listing Work Records</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @work_records.each do |work_record| %>
      <tr>
        <td><%= link_to 'Show', work_record %></td>
        <td><%= link_to 'Edit', edit_work_record_path(work_record) %></td>
        <td><%= link_to 'Destroy', work_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Work record', new_work_record_path %>
